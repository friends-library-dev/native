{
  "name": "@friends-library/native",
  "description": "friends library native apps",
  "version": "0.0.1",
  "repository": "github:friends-library-dev/native",
  "author": "Jared Henderson <jared@netrivet.com>",
  "private": true,
  "scripts": {
    "env:en": "fldev tsnode ./env/set-lang.ts en",
    "env:es": "fldev tsnode ./env/set-lang.ts es",
    "xcode": "open ./ios/FriendsLibrary.xcworkspace/",
    "xcode:en": "npm run env:en && npm run xcode",
    "xcode:es": "npm run env:es && npm run xcode",
    "xcode:release:en": "npm run env:en -- --release && npm run xcode",
    "xcode:release:es": "npm run env:es -- --release && npm run xcode",
    "android:en": "npm run env:en && react-native run-android",
    "android:es": "npm run env:es && react-native run-android",
    "ios:en": "npm run env:en && react-native run-ios",
    "ios:es": "npm run env:es && react-native run-ios",
    "start": "cp -f ./index.app.js ./index.js && react-native start",
    "start:en": "npm run env:en && npm run start",
    "start:es": "npm run env:es && npm run start",
    "android:prep-release": "mkdir -p ./android/app/src/main/assets && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build:android:apk:en": "npm run env:en && cd ./android && ./gradlew clean && npm run android:prep-release && ./gradlew assembleRelease -x bundleReleaseJsAndAssets && npm run open-apk-dir",
    "build:android:apk:es": "npm run env:es && cd ./android && ./gradlew clean && npm run android:prep-release && ./gradlew assembleRelease -x bundleReleaseJsAndAssets && npm run open-apk-dir",
    "build:android:aab:en": "npm run env:en && cd ./android && ./gradlew clean && npm run android:prep-release && ./gradlew bundleRelease -x bundleReleaseJsAndAssets && npm run open-aab-dir",
    "build:android:aab:es": "npm run env:es && cd ./android && ./gradlew clean && npm run android:prep-release && ./gradlew bundleRelease -x bundleReleaseJsAndAssets && npm run open-aab-dir",
    "open-apk-dir": "open ./android/app/build/outputs/apk/release",
    "open-aab-dir": "open ./android/app/build/outputs/bundle/release",
    "storybook": "cp -f ./index.storybook.js ./index.js && react-native start",
    "test": "true",
    "compile": "true",
    "lint": "fldev lint",
    "lint:fix": "fldev lint:fix",
    "ts:check": "fldev ts:check",
    "format": "fldev format"
  },
  "dependencies": {
    "@friends-library/adoc-convert": "^1.2.0",
    "@friends-library/locale": "^1.0.0",
    "@friends-library/theme": "^1.1.0",
    "@friends-library/types": "^3.0.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.5",
    "@react-navigation/native": "^5.7.2",
    "@react-navigation/stack": "^5.8.0",
    "@reduxjs/toolkit": "^1.4.0",
    "@storybook/addon-actions": "^6.0.16",
    "@storybook/addon-knobs": "^6.0.16",
    "@storybook/addon-ondevice-actions": "^5.3.19",
    "@storybook/addon-ondevice-knobs": "^5.3.19",
    "filesize": "^6.1.0",
    "lodash.throttle": "^4.1.1",
    "p-limit": "^3.0.2",
    "react": "16.10.1",
    "react-native": "0.63.2",
    "react-native-fs": "^2.16.6",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-reanimated": "^1.10.1",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.9.0",
    "react-native-scrubber": "^1.1.5",
    "react-native-splash-screen": "^3.2.0",
    "react-native-swift": "^1.2.3",
    "react-native-track-player": "^1.2.3",
    "react-native-vector-icons": "^7.0.0",
    "react-redux": "7.2.1",
    "redux": "^4.0.5",
    "tailwind-rn": "^1.1.1",
    "x-ts-utils": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@friends-library/dev": "^4.1.12",
    "@react-native-community/eslint-config": "^1.1.0",
    "@storybook/addon-actions": "^6.0.16",
    "@storybook/addon-essentials": "^6.0.16",
    "@storybook/addon-links": "^6.0.16",
    "@storybook/addon-ondevice-actions": "^5.3.19",
    "@storybook/react-native": "^5.3.19",
    "@storybook/react-native-server": "^5.3.19",
    "@types/lodash.throttle": "^4.1.6",
    "@types/react-native": "^0.63.1",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-loader": "^8.1.0",
    "husky": "^4.3.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-dom": "16.10.1",
    "react-is": "16.10.1",
    "x-exec": "^1.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node ./.githooks/pre-commit.js"
    }
  }
}
